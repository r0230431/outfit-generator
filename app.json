{
  "expo": {
    "name": "My outfit generator",
    "slug": "My_outfit_generator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon-dark.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#f4f1eb"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vanrompaey.els.myoutfitgenerator",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "$(PRODUCT_NAME) needs access to your photo library to save the photos you take and allow you to pick photos from your library.",
        "NSPhotoLibraryAddUsageDescription": "$(PRODUCT_NAME) needs access to your photo library to save the photos you take.",
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to your Camera."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#2b2926"
      },
      "package": "com.vanrompaey.els.myoutfitgenerator",
      "permissions": [
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.MEDIA_LIBRARY",
        "android.permission.CAMERA",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#f4f1eb",
          "image": "./assets/images/splash-icon-dark.png",
          "dark": {
            "image": "./assets/images/splash-icon-light.png",
            "backgroundColor": "#2b2926"
          },
          "imageWidth": 200
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera."
        }
      ],
      "expo-sqlite",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "react-native-vision-camera"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b850fc6a-660b-42c0-af20-a58aacf657a6"
      }
    },
    "owner": "els_vr"
  }
}
